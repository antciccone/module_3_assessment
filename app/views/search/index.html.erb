<table style="width:100%">
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone</th>
    <th>Store Type</th>
  </tr>
  <%= @stores.each do |store| %>
  <tr>
    <td> <%= store.name %>  </td>
    <td> <%= store.city %>  </td>
    <td><%= store.distance %> </td>
    <td><%= store.phone %> </td>
    <td><%= store.type %> </td>
  </tr>
</table>

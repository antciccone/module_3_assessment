<h1>Stores within 25 Miles</h1>
<table style="width:100%">
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone</th>
    <th>Store Type</th>
  </tr>
  <%@stores.each do |store| %>
  <tr>
    <td> <%= link_to store.name, ("#{stores_path}""?store_id=#{store.id}") %>  </td>
    <td> <%= store.name %>  </td>
    <td> <%= store.city %>  </td>
    <td><%= store.distance %> </td>
    <td><%= store.phone %> </td>
    <td><%= store.type %> </td>
  </tr>
  <% end %>
</table>
